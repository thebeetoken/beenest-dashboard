{
  "users": {
    "title": "Users",
    "metrics": [
      {
        "label": "New users",
        "query": {
          "analysis_type": "count",
          "event_collection": "signup"          
        }
      },
      {
        "label": "New hosts",
        "query": {
          "analysis_type": "count",
          "event_collection": "signup",
          "filters": [
            {
              "property_name": "isHost",
              "operator": "eq",
              "property_value": true
            }
          ]
        }        
      },
      {
        "label": "New guests",
        "query": {
          "analysis_type": "count",
          "event_collection": "signup",
          "filters": [
            {
              "property_name": "isHost",
              "operator": "eq",
              "property_value": false
            }
          ]          
        }        
      }
    ]
  },
  "onboarding": {
    "title": "Onboarding",
    "metrics": [
      {
        "label": "Verified",
        "query": {
          "analysis_type": "count",
          "event_collection": "userVerified"
        }
      },
      {
        "label": "Stripe",
        "query": {
          "analysis_type": "count",
          "event_collection": "userPayoutInfoCompleted",
          "filters": [{
            "property_name": "stripeExpressAccount",
            "operator": "eq",
            "property_value": true
          }]
        }
      },
      {
        "label": "ETH wallet",
        "query": {
          "analysis_type": "count",
          "event_collection": "userPayoutInfoCompleted",
          "filters": [{
            "property_name": "ethWalletAddress",
            "operator": "eq",
            "property_value": true
          }]
        }
      },
      {
        "label": "BTC wallet",
        "query": {
          "analysis_type": "count",
          "event_collection": "userPayoutInfoCompleted",
          "filters": [{
            "property_name": "btcWalletAddress",
            "operator": "eq",
            "property_value": true
          }]
        }
      },
      {
        "label": "Profile pic",
        "query": {
          "analysis_type": "count",
          "event_collection": "userPayoutInfoCompleted",
          "filters": [{
            "property_name": "profilePhoto",
            "operator": "eq",
            "property_value": true
          }]
        }
      }
    ]
  },
  "bookings": {
    "title": "Bookings",
    "metrics": [
      {
        "label": "Started",
        "query": {
          "analysis_type": "count",
          "event_collection": "bookingStarted"
        }
      },
      {
        "label": "Requested",
        "query": {
          "analysis_type": "count",
          "event_collection": "bookingRequested"
        }
      },
      {
        "label": "Accepted",
        "query": {
          "analysis_type": "count",
          "event_collection": "bookingAccepted"
        }
      },
      {
        "label": "Rejected",
        "query": {
          "analysis_type": "count",
          "event_collection": "bookingRejected"
        }
      },
      {
        "label": "Rescinded",
        "query": {
          "analysis_type": "count",
          "event_collection": "bookingRescinded"
        }
      },
      {
        "label": "Cancelled",
        "query": {
          "analysis_type": "count",
          "event_collection": "bookingCancelled"
        }
      },
      {
        "label": "Paid by Guest",
        "query": {
          "analysis_type": "count",
          "event_collection": "bookingPaidByGuest"
        }
      },
      {
        "label": "Paid to Host",
        "query": {
          "analysis_type": "count",
          "event_collection": "bookingPaidOut"
        }
      }
    ]
  }
}